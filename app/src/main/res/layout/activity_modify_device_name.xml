<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.xdys.library.widget.TitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:left_drawable="@mipmap/arrow_left_b"
        app:right_content="@string/determine"
        app:right_text_color="@color/B33"
        app:right_text_size="14sp"
        app:show_left="true"
        app:show_right="true"
        app:title_content="@string/equipment_management"
        tools:layout_height="48dp" />

    <com.xdys.library.widget.DeleteEditText
        android:id="@+id/etSearchKey"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:paddingHorizontal="25dp"
        android:paddingVertical="17dp"
        android:singleLine="true"
        app:layout_constraintTop_toBottomOf="@id/titleBar" />
</androidx.constraintlayout.widget.ConstraintLayout>